{
  "name": "awesome-api",
  "version": "0.0.0",
  "apiVersion": "v1",
  "description": "API",
  "author": "awesome-project",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --config jest.config.json --no-cache",
    "test:watch": "jest --config jest.config.json --no-cache --watch",
    "test:cov": "jest --config jest.config.json --no-cache --coverage",
    "coveralls": "jest --config jest.config.json --coverage && cat ./coverage/lcov.info | coveralls",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "5.4.0",
    "@nestjs/core": "5.4.0",
    "@nestjs/swagger": "3.1.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.9.1",
    "reflect-metadata": "0.1.12",
    "rimraf": "2.6.2",
    "rxjs": "6.2.2",
    "swagger-ui-express": "4.0.7",
    "typescript": "3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "5.7.4",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.1",
    "@types/node": "10.7.1",
    "@types/supertest": "2.0.5",
    "coveralls": "3.0.5",
    "jest": "23.5.0",
    "nodemon": "1.18.3",
    "prettier": "1.14.2",
    "supertest": "3.1.0",
    "ts-jest": "23.1.3",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.5.0",
    "tslint": "5.11.0"
  }
}
